{
  "env": {
    "ZHINNX_ROOT": "site"
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/assets/(.*)", "destination": "/site/assets/$1" },
    { "source": "/public/(.*)", "destination": "/site/public/$1" },
    { "source": "/(.*\\..*)", "destination": "/site/$1" },
    { "source": "/(.*)", "destination": "/site/index.html" }
  ]
}
